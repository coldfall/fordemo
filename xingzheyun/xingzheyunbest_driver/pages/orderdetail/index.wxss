/* pages/orderdetail/index.wxss */


.rowinfo{
	margin: auto;
	width: 695rpx;
	height: 170rpx;
	margin-bottom: 15rpx;
	border-bottom: solid 1rpx #e0e0e0;

}

.rowinfo .colleft{
	width: 140rpx;
	height: 100%;
	float: left;
	display: flex;
	justify-content: center;
	align-items: center;
}

.head{
	width: 100rpx;
	height: 100rpx;
	border:solid 1rpx #e0e0e0;
	border-radius: 50rpx;
	text-align: center;
	display: flex;
	justify-content: center;
	align-items: center;
	overflow: hidden;
}

.head image{
	width: 100%;
	border-radius: 50rpx;
}


.rowinfo .colmiddle{
	width: 356rpx;
	height: 100%;
	float: left;	
}

.rowinfo .colmiddle .user{
	width: 100%;
	height: 82rpx;
	color: #333333;
	font-size: 40rpx;
	text-align: left;
	line-height: 82rpx;
}


.rowinfo .colmiddle .user .carnum{
	height: 47rpx;
	background-color: #b8b9c6;
	border-radius: 24rpx;
	text-align: center;
	padding-left: 20rpx;
	padding-right: 20rpx;
	color: #FFFFFF;
	line-height: 47rpx;
	font-size: 28rpx;
}

.rowinfo .colmiddle .userinfo{
	width: 100%;
	height: 65rpx;
	color: #b8b9c6;
	font-size: 26rpx;
	text-align: left;
	line-height: 32rpx;
}


.rowinfo  .colright{
	width:197rpx;
	height: 165rpx;
	float: left;
	display: flex;
	justify-content: center;
	align-items: center;
}

.rowinfo  .colright .btnmessage{
	padding: 0rpx;
	width: 61rpx;
	height: 46rpx;
	background-color: transparent;
	background-image: url('data:image/png;base64,');
	background-repeat: no-repeat;
	background-size: 61rpx 46rpx;
	float: left;
	margin-right: 20rpx;

}


.rowinfo  .colright .btnmobile{
	padding: 0rpx;
	width: 53rpx;
	height:53rpx;
	background-color: transparent;
	background-image: url('data:image/png;base64,');
	background-repeat: no-repeat;
	background-size: 53rpx 53rpx;
	float: left;

}


.content .orderrow{
	width: 98%;
	margin: auto;
}

.content .orderrow  .left{
	width: 22%;
	height: 100%;
	text-align: left;
	float: left;
}


.content .orderrow  .right{
	float: left;
	width: 78%;
	height: 100%;
	text-align: left;
}

.content .orderrow  .right .location{
	margin-left: 475rpx;
	position: absolute;
	background-image: url('data:image/png;base64,');
	width: 32rpx;
	height: 32rpx;
	margin-top: 20rpx;
}

.content .mapmain{
	position: relative;
	width: 750rpx;
	height: 994rpx;
	margin-left: -30rpx;
}

.content .rowbtn{
	position: fixed;
	bottom: 35rpx;
	text-align: center;
	left: 0rpx;

}

.content .rowprogress{
	position: fixed;
	width: 690rpx;
	height: 180rpx;
	border-radius: 10rpx;
	margin-left: -345rpx;
	left: 50%;
	bottom: 192rpx;
	background-color: #FFFFFF;
}

.probarback{
	position: relative;
	margin: auto;
	width: 518rpx;
	height: 2rpx;
	background-color: #e9ebf2;
	margin-top: 93rpx;

}

.probarback .probar{
	position: absolute;
	float: left;
	height: 2rpx;
	
	background-color: #3d73dd;

}

.probarback .probar.p1{
	width: 0rpx;
}

.probarback .probar.p2{
	width: 173rpx;
}

.probarback .probar.p3{
	width: 347rpx;
}
.probarback .probar.p4{
	width: 518rpx;
}

.probarback .steps{
	position: absolute;
	width: 105rpx;
	height: 92rpx;
	margin-top: -45rpx;
	text-align: center;
}

.probarback .steps.step1{
	margin-left: 0rpx;
}
.probarback .steps.step2{
	margin-left: 173rpx;
}
.probarback .steps.step3{
	margin-left: 347rpx;
}
.probarback .steps.step4{
	margin-left: 518rpx;
}

.probarback .steps .time{
	position: absolute;
	font-size: 26rpx;
	color: #b8b9c6;
	height: 36rpx;
	width: 105rpx;
	line-height: 36rpx;
	margin-left: -52rpx;

}
.probarback .steps .title{
	position: absolute;
	font-size:30rpx;
	color: #b8b9c6;
	height: 36rpx;
	width: 105rpx;
	line-height: 36rpx;
	margin-left: -52rpx;
	margin-top: 64rpx;

}

.probarback .steps .dot{
	position: absolute;
	width: 17rpx;
	height: 17rpx;
	border-radius: 9rpx;
	border:solid 4rpx #b8b9c6;
	margin-top: 38rpx;
	margin-left: -8rpx;
	background-color: #FFFFFF;
}

.probarback .steps.off .dot{
	border:solid 2rpx #3d73dd;
}

.probarback .steps.on .dot{
	border:solid 6rpx #3d73dd;
}

.probarback .steps.on .title{
	color: #333333;
}


.probarback .steps.on .time{
	color: #3d73dd;
}

.bigbutton.btnredirect{
	width: 40%;
	float: left;
	  color: #4678de;
	  background-color: #f2f5ff;
	  margin-left: 50rpx;
	  border:solid 1rpx #3d73dd;
}


.bigbutton.btntest{
	width: 40%;
	float: right;
	margin-right: 50rpx;
}

.reachhint {
	position: fixed;
	z-index: 1000;
	width: 100%;
	height: 100%;
}

.reachhint .hintpanel{
	position: absolute;
	width: 576rpx;
	height: auto;
	border-radius: 10rpx;
	background-color: #FFFFFF;
	text-align: center;
	padding: 50rpx;
	padding-left: 20rpx;
	padding-right: 20rpx;

	top: 179rpx;
	margin-left: -288rpx;
	left: 50%;

}

.reachhint .hintpanel .title{
	width: 100%;
	height: 70rpx;
	font-size: 48rpx;
	color: #000000;
	text-align: center;
	line-height: 70rpx;

}
.reachhint .hintpanel .smtitle{
	width: 100%;
	height: 70rpx;
	font-size: 30rpx;
	color: #333333;
	text-align: center;
	line-height: 70rpx;

}

.reachhint .hintpanel .lists{
	width: 100%;
	height: auto;
	font-size: 28rpx;

	color: #666666;
	text-align: left;
}


.reachhint .hintpanel .lists .listitem{
	width: auto;
	height: 60rpx;
	font-size: 28rpx;
	line-height: 60rpx;
	padding-left: 30rpx;
	padding-right: 30rpx;
	color: #666666;
	text-align: center;
	float: left;
	border-radius: 30rpx;
	background-color: #faf9ff;
	border:solid 2rpx #f4f3f6;
	margin-right: 20rpx;
	margin-bottom: 20rpx;
}

.reachhint .hintpanel .clear{
	width: 100%;
	height: 1rpx;
	clear: both;
	float: none;
}
.reachhint .hintpanel .smalltip{
	width: 100%;
	height: auto;
	font-size: 26rpx;
	color: #999999;
	text-align: center;
	float: none;
	margin-top: 20rpx;
	margin-bottom: 20rpx;
}

.reachhint .hintpanel .btnconfirm{
	width: 362rpx;
	height: 88rpx;
	font-size: 32rpx;
	line-height: 88rpx;
	background-color: #3d73dd;
	color: #ffffff;
	text-align: center;
	margin: auto;
}

