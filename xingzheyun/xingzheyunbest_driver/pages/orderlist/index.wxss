/* pages/orderlist/index.wxss */

.navis{
	width: 100%;
	height: 82rpx;
}

.navis .naviitem{
	width: 259rpx;
	float: left;
	text-align: left;

}
.navis .naviitem navigator{
	height: 82rpx;
	display: table-cell;
	vertical-align: bottom;
}
.navis .naviitem navigator.off{
	font-size: 30rpx;
	color: #b8b9c6;
}
.navis .naviitem navigator.on{
	font-size: 40rpx;
	color: #333333;
	background-image: url("data:image/png;base64,");
	background-size: 79rpx 21rpx;
	background-position: left bottom;
	background-repeat: no-repeat;
}

.listwrapper{
	width: 100%;
	height: 100%;
	margin-top: 64rpx;
}
.listwrapper .order{
	width: 100%;
	height: 897rpx;

}

.listwrapper .order .ordertime{
	width: 100%;
	height: 48rpx;
}

.listwrapper .order .ordertime .date{
	display: block;
	float: left;
	width: 87rpx;
	height: 48rpx;
	text-align: center;
	color: #333333;
	font-size: 48rpx;
	line-height: 48rpx;
	border-right: solid 1rpx #b8b9c6; 
}

.listwrapper .order .ordertime .year{
	display: block;
	float: left;	
	width: 300rpx;
	height: 48rpx;
	padding-left: 10rpx;
	text-align: left;
	color: #b8b9c6;
	font-size: 24rpx;
	line-height: 100%;

}

.listwrapper .order .ordertime .time{
	width: 200rpx;
	height: 48rpx;
	text-align: right;
	color: #b8b9c6;
	font-size: 30rpx;
	line-height: 48rpx;
	display: block;
	float: right;
	
}

.listwrapper .order .orderdetail{
	width: 100%;
	height: 90rpx;
	margin-top: 25rpx;
}

.orderdetail .statusbar{
	width: 100%;
	height: 91rpx;
	overflow:hidden;
	background-color: #f6f7fa;
	border-top-left-radius: 10rpx;
	border-top-right-radius: 10rpx;
	position: absolute;
}


.orderdetail .statusbar .ordertag{
	position: absolute;
	width: 160rpx;
	height: 160rpx;
	font-size: 24rpx;
	color: #ffffff;
	padding-top: 120rpx;
	text-align: center;
	transform:rotate(-45deg);
	margin-left: -92rpx;
	margin-top: -92rpx;
	background-color: #3d73dd;
	-webkit-box-sizing: border-box;
    box-sizing: border-box;

}

.orderdetail .statusbar .ordertitle{
	margin-left: 82rpx;
	width: auto;
	height: 91rpx;
	color: #333333;
	font-size: 32rpx;
	line-height: 91rpx;
	font-weight: bold;
	margin-right: 10rpx;
}

.orderdetail .statusbar .ordertype{
	width: 100rpx;
	height: 36rpx;
	border-radius: 5rpx;
	border:solid 2rpx #3d73dd;
	text-align: center;
	font-size: 24rpx;
	color: #3d73dd;
	padding-left: 10rpx;
	padding-right: 10rpx;
	line-height: 36rpx;
	margin-top: 27rpx;
}


.orderdetail .statusbar .orderstatus{
	width: 134rpx;
	height: 91rpx;
	text-align: center;
	font-size: 28rpx;
	color: #b8b9c6;
	line-height: 91rpx;
	float: right;
}

.noorder{
	width: 100%;
	height: 390rpx;
	text-align: center;
	color: #999999;
}

.orderpos{
	width: 100%;
	height: 190rpx;
	margin: auto;
	margin-top: 20rpx;
	padding-bottom: 20rpx;
	border-bottom: solid 1rpx #eeeeee;
}

.orderpos .pos{
	width: 100%;
	height: 90rpx;
	overflow:hidden;

}

.orderpos .pos .icon{
	width: 71rpx;
	height: 90rpx;
	float: left;
	vertical-align: middle;
	text-align: center;
	background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAIAAAAJCAIAAACXE2qwAAAAGXRFWHRTb2Z0d2FyZQBBZG9iZSBJbWFnZVJlYWR5ccllPAAAABdJREFUeNpi/Pb9FwMDAxMDGJBMAQQYALN9Avngy4+HAAAAAElFTkSuQmCC');
	background-size: 2rpx 30rpx;
	background-repeat: no-repeat;
	background-position: center middle;
	line-height: 90rpx;
}

.orderpos .pos.posstart .icon image{
	width: 22rpx;
}	

.orderpos .pos.posend .icon image{
	width: 31rpx;
}	


.orderpos .pos .distance{
	width: 113rpx;
	height: 90rpx;
	float: left;
	vertical-align: middle;
	text-align: left;
	color: #b8b9c6;
	font-size: 28rpx;
	line-height: 100%;
	line-height: 90rpx;
}


.orderpos .pos .address{
	width: 465rpx;
	height: 90rpx;
	float: left;
	vertical-align: middle;
	text-align: left;
	color: #333333;
	font-size: 30rpx;
	line-height: 90rpx;	
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.orderpos .pos.posstart .icon{
	background-position: center bottom;
}

.orderpos .pos.posend .icon{
	background-position: center top;
}

.content .inforow{
	width: 90%;
	margin: auto;
}

.content .inforow  .left{
	width: 30%;
	height: 100%;
	text-align: left;
	float: left;
}


.content .inforow  .right{
	float: right;
	width: 70%;
	height: 100%;
	text-align: right;
}

.content .messagerow{
	width: 640rpx;
	height: 80rpx;
	margin: auto;
	text-align: left;
	font-size: 28rpx;
	line-height: 80rpx;
	color: #333333;
	padding-left: 65rpx;
	background-image: url('data:image/png;base64,');
	background-repeat: no-repeat;
	background-position: 15rpx center;
	background-size: 30rpx 30rpx;
	background-color: #f6f7fa;
	border-radius: 10rpx;
	margin-top: 20rpx;
	margin-bottom: 20rpx;
}

.servicerow{
	width: 100%;
	height: 65rpx;
	margin-top: 20rpx;

}

.servicerow .smallbutton{
	float: right;
	margin-left: 15rpx;
}

.servicerow .smallbutton.detailbtn{
	width: 200rpx;
	border:solid 2rpx #3d73dd;
}