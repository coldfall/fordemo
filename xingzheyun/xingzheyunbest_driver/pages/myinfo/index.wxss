/* pages/myinfo/index.wxss */

.navis{
	width: 100%;
	height: 82rpx;
}

.navis .naviitem{
	width: 259rpx;
	float: left;
	text-align: left;

}
.navis .naviitem navigator{
	height: 82rpx;
	display: table-cell;
	vertical-align: bottom;
}
.navis .naviitem navigator.off{
	font-size: 30rpx;
	color: #b8b9c6;
}
.navis .naviitem navigator.on{
	font-size: 40rpx;
	color: #333333;
	background-image: url("data:image/png;base64,");
	background-size: 79rpx 21rpx;
	background-position: left bottom;
	background-repeat: no-repeat;
}

.rowhead{
	width: 200rpx;
	height: 200rpx;
	border-radius: 100rpx;
	border:solid 1rpx #eeeeee;
	margin: auto;
	margin-top: 68rpx;
	background-color: #faf9ff;
	overflow: hidden;

}

.rowhead image{
	width: 100%;
}

.rowname{
	font-size: 40rpx;
}

.rowname .gender{
	width: 25rpx;
	margin-top: 20rpx;
}

.content .rowdesc{
	height: 50rpx;
	line-height: 50rpx;
}

.content .rowcomment{
	position: relative;
	width: 326rpx;
	height: 37rpx;
	margin: auto;
	font-size: 26rpx;
	color: #b8b9c6;
}

.content .rowcomment view{
	float: left;
}

.content .rowcomment .stars{
	width: 180rpx;
	height: 37rpx;

}
.content .rowcomment .stars image{
	width: 22rpx;
	margin-right: 5rpx;
	margin-left: 5rpx;
}

.content .rowcomment .score{
	width: 49rpx;
	color: #ffa200;

}
.content .rowservice{
	margin-top: 28rpx;
}
.content .rowservice .con{
	width: 637rpx;
	height: 69rpx;
	display:inline-block;
	margin: auto;
}

.content .rowservice button{
	float: left;
	width: 190rpx;
	margin-left: 10rpx;
	margin-right: 10rpx;
}

.content .rownob{
	margin: auto;
	width: 80%;
	line-height: 120rpx;
	
}

.content .rownob.firstrow{
	margin-top: 50rpx;
	
}

.content .rownob .colmobile{
	width: 40%;
	height: 100%;
	background-image: url('data:image/png;base64,');
	background-size: 18rpx 28rpx;
	background-repeat: no-repeat;
	background-position: left center;
	padding-left: 44rpx;

	text-align: left;
	float: left;
}

.content .rownob .colmobilenum{
	width: 60%;
	height: 100%;
	float: right;
	text-align: right;
}
.content .rownob .colidcard{
	width: 40%;
	height: 100%;
	background-image: url('data:image/png;base64,');
	background-size: 30rpx 23rpx;
	background-repeat: no-repeat;
	background-position: left center;
	padding-left: 44rpx;
	text-align: left;
	float: left;
}

.content .rownob .colidnum{
	width: 60%;
	height: 100%;
	float: right;
	text-align: right;
}


.content .rownob.endrow{
	margin-top: 100rpx;
	
}
.content .rownob.endrow button{
	padding: 0rpx;
	background:none;
	
}
.content .rownob.endrow{
	margin-top: 100rpx;
	
}

.content .rownob .colcar{
	width: 40%;
	height: 100%;
	text-align: left;
	float: left;
}

.content .rownob .rightarrow{

	float: right;
	text-align: right;
	margin-right: 10rpx;
	margin-top: 28rpx;
}







