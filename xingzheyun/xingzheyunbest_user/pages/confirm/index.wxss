/* pages/main/index.wxss */

@import "../templates/navigator.wxss";

.mapmain{
	position: absolute;
	width: 100%;
	height: 100%;
}

.swp{
	position: absolute;
	width: 100%;
	height: 100%;
}

.container{
	position: absolute;
	padding: 30rpx;
}



.callinfo{
	width: 690rpx;
	height: 196rpx;
	background-color: #FFFFFF;
	margin: auto;
	border-radius: 10rpx;
	
	background-repeat: no-repeat;
	background-position: 647rpx center;

}

.callinfo .info{
	width: 90%;
	height: 50%;
	margin: auto;

	float: none;
}

.callinfo .info .infoicon{
	width: 49rpx;
	height:  100%;
	text-align: center;
	display: flex;
	justify-content: center;
	align-items: center;
	float: left;
}

.callinfo .info .infoicon image{
	width: 28rpx;
}


.callinfo .info .infotitle{
	width: 139rpx;
	height:  100%;
	text-align: left;
	color: #b8b9c6;
	font-size: 28rpx;
	line-height: 98rpx;
	float: left;
	padding-left: 10rpx;
}

.callinfo .info .infotxt{
	width: 427rpx;
	height:  100%;
	text-align: left;
	padding-left: 5rpx;
	color: #333333;
	font-size: 28rpx;
	line-height: 98rpx;
	float: left;
	display: block;
}
.callinfo .info .infotxt input{
	width: 100%;
	height:  100%;
	text-align: left;
	padding-left: 5rpx;
	color: #333333;
	font-size: 28rpx;
	line-height: 98rpx;

	display: block;
}


.container .rownob.rowbtn{
	width: 750rpx;
	position: fixed;
	bottom: 70rpx;
	margin-left: -375rpx;
	left: 50%;
}

.panelfee{
	width: 690rpx;
	height: 353rpx;
	margin: auto;
	background-image: url('data:image/png;base64,');
	position: fixed;
	bottom: 218rpx;
	padding-left: 26rpx;
	padding-right: 26rpx;
	padding-top: 23rpx;
	padding-bottom: 23rpx;	
}

.panelfee .rowtitle{
	width: 636rpx;
	height: 81rpx;
	margin: auto;
	padding: 13rpx;
	margin-bottom: 51rpx;
}

.panelfee .rowtitle .icon{
	width: 62rpx;
	height: 62rpx;
	border-radius: 31rpx;
	background-color: #f2f5ff;
	text-align: center;
	line-height: 62rpx;
	overflow: hidden;
	float: left;

}

.panelfee .rowtitle .icon image{
	width: 39rpx;
}

.panelfee .rowtitle .title{
	width: 160rpx;
	height: 62rpx;
	line-height: 62rpx;
	float: left;
	margin-left: 10rpx;
	text-align: left;
	color: #333333;
	font-size: 32rpx;

}

.panelfee .rowtitle .time{
	width: 373rpx;
	height: 62rpx;
	line-height: 62rpx;
	float: right;

	text-align: right;
	color: #b8b9c6;
	font-size: 28rpx;

}

.panelfee .feeinfo{
	width: 626rpx;
	height: 54rpx;
	margin: auto;
	font-size: 28rpx;
}
.panelfee .feeinfo .left{
	width: 70%;
	height: 54rpx;
	float: left;
	text-align: left;
	color: #b8b9c6;
}
.panelfee .feeinfo .right{
	width: 30%;
	height: 54rpx;
	float: left;
	text-align: right;
	color: #333333;
}

.panelfee .fee{
	width: 626rpx;
	height: 60rpx;
	margin: auto;
	color: #333333;
	font-size: 30rpx;
	line-height: 60rpx;
	text-align: right;
}

.panelfee .fee .money{
	font-size: 26rpx;
	color: #3d73dd;
}

.panelfee .fee .num{
	font-size: 36rpx;
	color: #3d73dd;
}