/* pages/main/index.wxss */

@import "../templates/navigator.wxss";

.mapmain{
	position: absolute;
	width: 100%;
	height: 100%;
}

.swp{
	position: absolute;
	width: 100%;
	height: 100%;
}

.container{
	position: absolute;
	padding: 30rpx;
}

.container .title{
	width: 100%;
	height: 50rpx;
	line-height: 50rpx;
	font-size: 43rpx;
	text-align: left;
	color: #333333;
	font-weight: bold;
}

.floathint{
	width: 690rpx;
	height: 90rpx;
	margin-top: 30rpx;
	margin-bottom: 60rpx;
	border-radius: 10rpx;
	background-color: #FFFFFF;


}

.floathint .caricon{
	width: 67rpx;
	height: 67rpx;
	border-radius: 34rpx;
	background-color: #f2f2f2;
	background-image: url('data:image/png;base64,');
	background-position: center center;
	background-size: 34rpx 27rpx;
	background-repeat: no-repeat;
	float: left;
	margin-left: 17rpx;
	margin-top: 12rpx;
}

.floathint .hintword{
	width: 540rpx;
	height: 90rpx;
	text-align: left;
	font-size: 26rpx;
	color: #333333;
	line-height: 90rpx;
	margin-left: 20rpx;
	float: left;
}

.floathint .hintword .red{
	color: #FF0000;
	font-size: 32rpx;
}

.floathint .arrow{

	margin-right: 23rpx;
	margin-top: 34rpx;
	float: right;
}

.services{
	width: 100%;
	height: 620rpx;
	margin-top: 40rpx;
}

.services .servbtn{
	width: 326rpx;
	height: 126rpx;

}

.services .servbtn.btnleft{
	float: left;
	margin-left: 0rpx;
	
}
.services .servbtn.btnright{
	float: right;
	margin-right: 0rpx;
	
}

.services .servbtn .circle{
	width: 110rpx;
	height: 110rpx;
	border-radius: 55rpx;
	background-color: #3d73dd;
	position: absolute;
	display: flex;
	justify-content: center;
	align-items: center;
	box-shadow: 0rpx 0rpx 10rpx #b6c1e8;
}

.services .servbtn .circle image{
	width: 55rpx;
	height: 55rpx;
}

.services .servbtn .cap{
	width: 269rpx;
	height: 90rpx;
	border-radius: 45rpx;
	background-color: #FFFFFF;
	text-align: left;
	padding-left: 95rpx;
	color: #333333;
	font-size: 30rpx;
	line-height: 90rpx; 
	margin-top: 10rpx;
	margin-left: 40rpx;
	box-shadow: 0rpx 0rpx 10rpx #d4d4d4;

}


.callinfo{
	width: 690rpx;
	height: 196rpx;
	background-color: #FFFFFF;
	margin: auto;
	border-radius: 10rpx;
	background-image: url('data:image/png;base64,');
	background-size: 13rpx 24rpx;
	background-repeat: no-repeat;
	background-position: 647rpx center;

}

.callinfo .info{
	width: 90%;
	height: 50%;
	margin: auto;

	float: none;
}

.callinfo .info .infoicon{
	width: 49rpx;
	height:  100%;
	text-align: center;
	display: flex;
	justify-content: center;
	align-items: center;
	float: left;
}

.callinfo .info .infoicon image{
	width: 28rpx;
}


.callinfo .info .infotitle{
	width: 139rpx;
	height:  100%;
	text-align: left;
	color: #b8b9c6;
	font-size: 28rpx;
	line-height: 98rpx;
	float: left;
}

.callinfo .info .infotxt{
	width: 427rpx;
	height:  100%;
	text-align: left;
	padding-left: 5rpx;
	color: #333333;
	font-size: 28rpx;
	line-height: 98rpx;
	float: left;
	display: block;
}


.container .rownob.rowbtn{
	width: 750rpx;
	position: fixed;
	bottom: 70rpx;
	margin-left: -375rpx;
	left: 50%;
}

.panelsuc{
	position: fixed;
	width: 100%;
	height: 100%;
	z-index: 999999;
	animation: pani 0.5s linear;
}

@keyframes pani{
	0%{opacity: 0;}
	100%{opacity: 1;}
}




.panelsuc .sucwrapper{
	position: relative;
	width: 660rpx;
	height: 480rpx;
	margin: auto;
	margin-top: 572rpx;
}

.panelsuc .sucwrapper .kv{
	width: 660rpx;
}

.panelsuc .sucwrapper .btnconfirm{
	width: 362rpx;
	height: 88rpx;
	border-radius: 44rpx;
	margin: auto;
	margin-top: 32rpx;
	text-align: center;
	color: #FFFFFF;
	font-size: 30rpx;
	line-height: 88rpx;
	background-color: #3d73dd;

}