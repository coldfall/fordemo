/* pages/mycars/index.wxss */
@import "../templates/navigator.wxss";
.rowcars{
	margin: auto;
	width: 695rpx;
	height: 155rpx;
	margin-top: 15rpx;
	margin-bottom: 15rpx;
}

.rowcars .colleft{
	width: 158rpx;
	height: 100%;
	float: left;
	display: flex;
	justify-content: center;
	align-items: center;
}

.carlogo{
	width: 100rpx;
	height: 100rpx;
	border:solid 1rpx #e0e0e0;
	border-radius: 50rpx;
	text-align: center;
	display: flex;
	justify-content: center;
	align-items: center;
	overflow: hidden;
}

.carlogo image{
	width: 100%;
	border-radius: 50rpx;
}


.rowcars .colmiddle{
	width: 270rpx;
	height: 100%;
	float: left;	
}

.rowcars .colmiddle .carnum{
	width: 270rpx;
	height: 52rpx;
	color: #333333;
	font-size: 36rpx;
	text-align: left;
	line-height: 52rpx;
	margin-top: 35rpx;
}

.rowcars .colmiddle .cardetail{
	width: 270rpx;
	height: 28rpx;
	color: #b8b9c6;
	font-size: 28rpx;
	text-align: left;
	line-height: 28rpx;
}


.rowcars  .colright{
	width:258rpx;
	height: 155rpx;
	background-image: url('data:image/jpeg;base64,');
	float: left;
	display: flex;
	justify-content: center;
	align-items: center;
	background-repeat: no-repeat;
	background-size: contain;	
}

.rowcars  .colright .btnedit{
	padding: 0rpx;
	width: 110rpx;
	height: 28rpx;
	background-color: transparent;
	background-image: url('data:image/png;base64,');
	background-repeat: no-repeat;
	background-size: 22rpx 22rpx;
	background-position: left center;
	padding-left: 30rpx;
	margin-left: 18rpx;
	float: left;
	color: #b8b9c6;
	font-size: 26rpx;
	line-height: 28rpx;
	text-align: left;


}


.rowcars  .colright .btndelete{
	padding: 0rpx;
	width: 110rpx;
	height: 28rpx;
	background-color: transparent;
	background-image: url('data:image/png;base64,');
	background-repeat: no-repeat;
	background-size: 25rpx 25rpx;
	background-position: left center;
	margin-left: 18rpx;
	padding-left: 30rpx;
	float: left;
	color: #b8b9c6;
	font-size: 26rpx;
	line-height: 28rpx;
	text-align: left;

}

.content .rownob.rowbtn{
	position: fixed;
	bottom: 220rpx;
	margin-left: -375rpx;
	left: 50%;
}