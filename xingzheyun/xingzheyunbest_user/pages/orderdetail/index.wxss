/* pages/orderdetail/index.wxss */
.mapmain{
	position: absolute;
	width: 100%;
	height: 100%;
}
.content{
	position: absolute;
	width: 100%;
	height: 100%;
	padding: 0rpx;
}

.rowinfo{
	margin: auto;
	width: 100%;
	height: 165rpx;
	background-color: #FFFFFF;


}

.rowinfo .colleft{
	width: 180rpx;
	height: 100%;
	float: left;
	display: flex;
	justify-content: center;
	align-items: center;
}

.head{
	width: 100rpx;
	height: 100rpx;
	border:solid 1rpx #e0e0e0;
	border-radius: 50rpx;
	text-align: center;
	display: flex;
	justify-content: center;
	align-items: center;
	overflow: hidden;
}

.head image{
	width: 100%;
	border-radius: 50rpx;
}


.rowinfo .colmiddle{
	width: 386rpx;
	height: 100%;
	float: left;	
}

.rowinfo .colmiddle .user{
	width: 100%;
	height: 82rpx;
	color: #333333;
	font-size: 40rpx;
	text-align: left;
	line-height: 82rpx;
}


.rowinfo .colmiddle .user .carnum{
	height: 47rpx;
	background-color: #b8b9c6;
	border-radius: 24rpx;
	text-align: center;
	padding-left: 20rpx;
	padding-right: 20rpx;
	color: #FFFFFF;
	line-height: 47rpx;
	font-size: 28rpx;
}


.content .rowcomment{
	position: relative;
	width: 326rpx;
	height: 37rpx;
	text-align: left;
	font-size: 26rpx;
	color: #b8b9c6;
}

.content .rowcomment view{
	float: left;
}

.content .rowcomment .stars{
	width: 180rpx;
	height: 37rpx;

}
.content .rowcomment .stars image{
	width: 22rpx;
	margin-right: 5rpx;
	margin-left: 5rpx;
}

.content .rowcomment .score{
	width: 49rpx;
	color: #ffa200;

}

.rowinfo .colmiddle .userinfo{
	width: 100%;
	height: 65rpx;
	color: #b8b9c6;
	font-size: 28rpx;
	text-align: left;
	line-height: 28rpx;
}


.rowinfo  .colright{
	width:167rpx;
	height: 165rpx;
	float: left;
	display: flex;
	justify-content: center;
	align-items: center;
}

.rowinfo  .colright .btnmessage{
	padding: 0rpx;
	width: 61rpx;
	height: 46rpx;
	background-color: transparent;
	background-image: url('data:image/png;base64,');
	background-repeat: no-repeat;
	background-size: 61rpx 46rpx;
	float: left;
	margin-right: 20rpx;

}


.rowinfo  .colright .btnmobile{
	padding: 0rpx;
	width: 53rpx;
	height:53rpx;
	background-color: transparent;
	background-image: url('data:image/png;base64,');
	background-repeat: no-repeat;
	background-size: 53rpx 53rpx;
	float: left;

}


.content .orderrow{
	width: 98%;
	margin: auto;
}

.content .orderrow  .left{
	width: 22%;
	height: 100%;
	text-align: left;
	float: left;
}


.content .orderrow  .right{
	float: left;
	width: 78%;
	height: 100%;
	text-align: left;
}

.content .orderrow  .right .location{
	margin-left: 475rpx;
	position: absolute;
	background-image: url('data:image/png;base64,');
	width: 32rpx;
	height: 32rpx;
	margin-top: 20rpx;
}


.content .rowbtn{
	position: fixed;
	bottom: 40rpx;
	text-align: center;
	left: 0rpx;

}

.content .rowprogress{
	position: fixed;
	width: 690rpx;
	height: 196rpx;
	border-radius: 15rpx;
	margin-left: -345rpx;
	left: 50%;
	bottom: 203rpx;
	background-color: #FFFFFF;
}

.probarback{
	position: relative;
	margin: auto;
	width: 554rpx;
	height: 2rpx;
	background-color: #e9ebf2;
	margin-top: 93rpx;

}

.probarback .probar{
	position: absolute;
	float: left;
	height: 2rpx;
	
	background-color: #3d73dd;

}

.probarback .probar.p0{
	width: 0rpx;
}

.probarback .probar.p1{
	width: 137rpx;
}

.probarback .probar.p2{
	width: 276rpx;
}
.probarback .probar.p3{
	width: 414rpx;
}
.probarback .probar.p4{
	width: 554rpx;
}


.probarback .steps{
	position: absolute;
	width: 105rpx;
	height: 102rpx;
	margin-top: -45rpx;
	text-align: center;
}

.probarback .steps.step1{
	margin-left: 0rpx;
}
.probarback .steps.step2{
	margin-left: 137rpx;
}
.probarback .steps.step3{
	margin-left:276rpx;
}
.probarback .steps.step4{
	margin-left: 414rpx;
}
.probarback .steps.step5{
	margin-left: 554rpx;
}


.probarback .steps .time{
	position: absolute;
	font-size: 26rpx;
	color: #b8b9c6;
	height: 36rpx;
	width: 105rpx;
	line-height: 36rpx;
	margin-left: -52rpx;

}
.probarback .steps .title{
	position: absolute;
	font-size:30rpx;
	color: #b8b9c6;
	height: 36rpx;
	width: 105rpx;
	line-height: 36rpx;
	margin-left: -52rpx;
	margin-top: 64rpx;

}

.probarback .steps .dot{
	position: absolute;
	width: 17rpx;
	height: 17rpx;
	border-radius: 9rpx;
	border:solid 4rpx #b8b9c6;
	margin-top: 38rpx;
	margin-left: -8rpx;
	background-color: #FFFFFF;
}

.probarback .steps.off .dot{
	border:solid 2rpx #3d73dd;
}

.probarback .steps.on .dot{
	border:solid 6rpx #3d73dd;
}

.probarback .steps.on .title{
	color: #333333;
}


.probarback .steps.on .time{
	color: #3d73dd;
}

.btncancel{
  width: 270rpx;
  height: 98rpx;
  text-align: center;
  font-size: 30rpx;
  color: #FFFFFF;
  line-height: 98rpx;
  background-color: #b8b9c6;
  border-radius: 10rpx;
  float: left;
  margin-left: 30rpx;

}

.btnpush{
  width: 400rpx;
  height: 98rpx;
  text-align: center;
  font-size: 30rpx;
  color: #FFFFFF;
  line-height: 98rpx;
  background-color: #3d73dd;
  border-radius: 10rpx;
  float: left;
  margin-left: 23rpx;

}

.commentwrapper{
	width: 100%;
	height: 100%;
}

.commentwrapper .comment{
	width: 100%;
	height: 559rpx;
	background-color: #FFFFFF;
	border-radius: 10rpx;
	position: absolute;
	bottom: 0rpx;
}

.commentwrapper .comment .title{
	width: 100%;
	height: 100rpx;
	text-align: center;
	color: #333333;
	font-size: 30rpx;
	line-height: 100rpx;
}

.commentwrapper .comment .btnclose{
	position: absolute;
	right: 20rpx;
	margin-top: 20rpx;
	width: 46rpx;
	height: 46rpx;
	background-image: url('data:image/png;base64,');
	background-size: contain;
}

.commentwrapper .stars{
	width: 391rpx;
	height: 64rpx;
	margin: auto;
}

.commentwrapper .stars image{
	width: 46rpx;
	float: left;
	margin-right: 34rpx;
}

.commentwrapper .stars image:last-child{
	margin-right: 0rpx;
}

.commentwrapper .starsword{
	width: 100%;
	height: 57rpx;
	text-align: center;
	margin-bottom: 10rpx;
	color: #ffa200;
	font-size: 26rpx;
	line-height: 57rpx;
}